{% extends 'base.html.twig' %}

{% block title %}Societe{% endblock %}

{% block body %}
    <h1>Societe</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ societe.id }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ societe.logo }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ societe.nom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ societe.adresse }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ societe.tel }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ societe.fax }}</td>
            </tr>
            <tr>
                <th>SiteWeb</th>
                <td>{{ societe.siteWeb }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ societe.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('societe_index') }}">back to list</a>

    <a href="{{ path('societe_edit', {'id': societe.id}) }}">edit</a>

    {{ include('societe/_delete_form.html.twig') }}
{% endblock %}
